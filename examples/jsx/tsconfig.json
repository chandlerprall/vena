{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "noEmit": false,
    "outDir": "./build",
    "sourceMap": true,
    "declaration": false,

    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,

    "jsx": "react-jsx",
    "jsxImportSource": "vena",
    "paths": {
        "vena": ["../../build/index"],
        "vena/jsx-runtime": ["../../build/jsx-runtime"],
        "vena/jsx.d.ts": ["../../build/jsx.d.ts"]
    }
  },
//  "files": [
//    "src/jsx-app.tsx",
//
//  ],
  "include": [
    "src/**/*"
  ]
}